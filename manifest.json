{
	"name": "Spinnify",
	"description": "Make the divs on a page spin on hover",
	"version": "0.0",
	"permissions":[
		"http://*/*",
		"activeTab"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_title": "Spinnify",
		"default_icon": "spin.png"
	},
	"manifest_version": 2
}